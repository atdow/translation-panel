{
  "name": "translation-panel",
  "displayName": "translation panel",
  "description": "translation panel",
  "publisher": "atdow",
  "icon": "icon.png",
  "version": "1.1.1",
  "license": "MIT License",
  "repository": {
    "url": "https://github.com/atdow/translation-panel.git",
    "type": "git"
  },
  "engines": {
    "vscode": "^1.66.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.translationPanel.open",
        "title": "translation panel"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorFocus",
          "command": "extension.translationPanel.open",
          "group": "navigation@1"
        }
      ],
      "explorer/context": [
        {
          "command": "extension.translationPanel.open",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "dev": "webpack --mode development --config build/webpack.dev.config.js",
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js",
    "build": "webpack --mode production --config ./build/node-extension.webpack.config.js",
    "package": "vsce package --no-yarn"
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@babel/plugin-proposal-decorators": "^7.20.0",
    "@babel/preset-env": "^7.19.4",
    "@google-cloud/translate": "^6.0.4",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "14.x",
    "@types/vscode": "^1.66.0",
    "@vscode/test-electron": "^2.1.3",
    "babel-loader": "^9.0.0",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.7.1",
    "eslint": "^8.14.0",
    "file-loader": "^6.2.0",
    "glob": "^8.0.1",
    "html-webpack-plugin": "^4.5.0",
    "less": "^4.1.3",
    "less-loader": "^11.1.0",
    "mocha": "^9.2.2",
    "style-loader": "^3.3.1",
    "typescript": "^4.6.4",
    "url-loader": "^4.1.1",
    "vue": "^2.7.13",
    "vue-loader": "^17.0.1",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^5.74.0",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "axios": "0.21.0",
    "md5": "2.3.0",
    "uuid": "8.3.1"
  }
}