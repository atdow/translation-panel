{
  "name": "translation-panel",
  "displayName": "translation panel",
  "description": "translation panel",
  "publisher": "atdow",
  "icon": "icon.png",
  "version": "0.0.1",
  "license": "Apache-2.0",
  "repository": {
    "url": "https://github.com/Lwenkun/Translation",
    "type": "git"
  },
  "engines": {
    "vscode": "^1.66.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.translationPanel.open",
        "title": "translation panel"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorFocus",
          "command": "extension.translationPanel.open",
          "group": "navigation@1"
        }
      ],
      "explorer/context": [
        {
          "command": "extension.translationPanel.open",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@google-cloud/translate": "^6.0.4",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "14.x",
    "@types/vscode": "^1.66.0",
    "@vscode/test-electron": "^2.1.3",
    "axios": "^0.21.0",
    "eslint": "^8.14.0",
    "glob": "^8.0.1",
    "md5": "^2.3.0",
    "mocha": "^9.2.2",
    "typescript": "^4.6.4",
    "uuid": "^8.3.1"
  }
}